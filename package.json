{
  "name": "client",
  "version": "0.1.0",
  "proxy": "http://localhost:5555/",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.1",
    "co": "^4.6.0",
    "cookie-parser": "1.3.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "~4.16.0",
    "jsonwebtoken": "^8.3.0",
    "prop-types": "^15.6.2",
    "querystring": "~0.2.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-dropdown": "^1.4.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "request": "~2.83.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "node-less-chokidar": "^0.1.2",
    "npm-run-all": "^4.1.3",
    "react-scripts": "1.1.1",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "tailwindcss": "^0.6.4"
  },
  "scripts": {
    "build-css": "node-less-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-less-chokidar src/ -o src/ --watch --recursive",
    "tailwind:css": "tailwind build src/css/tailwind.src.css -c  tailwind.js -o src/css/tailwind.css",
    "start-js": "react-scripts start",
    "client": "npm-run-all -p tailwind:css watch-css start-js",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run tailwind:css && npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server": "node backend/serverRecommendations.js",
    "lint": "eslint ./src"
  }
}
